    "\n",
    "frame_width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n",
    "frame_height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n",
    "fps = cap.get(cv2.CAP_PROP_FPS)\n",
    "print(f\"FPS của video: {fps}\")\n",
    "out = None\n",
    "\n",
    "predicted_data = []\n",
    "cap.set(cv2.CAP_PROP_POS_FRAMES, 0)\n",
    "frame_idx = 0\n",
    "\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    second = round((frame_idx + 1) / fps, 2)\n",
    "    det_boxes = detection_boxes.get(frame_idx + 1, [])\n",
    "    boxes_to_draw = []\n",
    "    for box, color, text in det_boxes:\n",
    "        boxes_to_draw.append((box, color))\n",
    "        cv2.putText(frame, text, (int(box[0]), int(box[1]) - 10),\n",
    "                    cv2.FONT_HERSHEY_SIMPLEX, 0.5, color, 2)\n",
    "    for object_id, predictions in all_predictions.items():\n",
    "        if frame_idx < len(predictions):\n",
    "            pred_box = predictions[frame_idx]\n",
    "            if pred_box is not None:\n",
    "                pred_box_coords = [float(x) for x in pred_box[:4]]\n",
    "                boxes_to_draw.append((pred_box_coords, (0, 0, 255)))\n",
    "                label_text = f\"ID {object_id} - {object_labels.get(object_id, '')}\"\n",
    "                cv2.putText(frame, label_text, (int(pred_box_coords[0]), int(pred_box_coords[1]) - 10),\n",
    "                            cv2.FONT_HERSHEY_SIMPLEX, 0.5, (0, 0, 255), 2)\n",
    "                predicted_data.append({\n",
    "                    \"frame\": frame_idx + 1,\n",
    "                    \"second\": second,\n",
    "                    \"id\": object_id,\n",
    "                    \"label\": object_labels.get(object_id, \"\"),\n",
    "                    \"bbox\": \",\".join([str(int(x)) for x in pred_box_coords])\n",
    "                })\n",
    "\n",
    "    draw_boxes(frame, boxes_to_draw)\n",
    "    cv2.imshow(\"Detection (Green) & Prediction (Red)\", frame)\n",
    "    if out is None:\n",
    "        out = cv2.VideoWriter(\"output_video.mp4\", cv2.VideoWriter_fourcc(*\"mp4v\"), fps, (frame_width, frame_height))\n",
    "    out.write(frame)\n",
    "    if cv2.waitKey(int(1000 / fps)) & 0xFF == ord(\"q\"):\n",
    "        break\n",
    "    frame_idx += 1\n",
    "\n",
    "cap.release()\n",
    "if out is not None:\n",
    "    out.release()\n",
    "cv2.destroyAllWindows()\n",
    "\n",
    "predicted_df = pd.DataFrame(predicted_data, columns=[\"frame\", \"second\", \"id\", \"label\", \"bbox\"])\n",
    "predicted_df.to_csv(\"predicted_data.csv\", index=False)\n",
    "print(\"Đã xuất dữ liệu dự đoán ra file predicted_data.csv\")\n",
    "\n",
    "# ======= TÍNH ĐỘ CHÍNH XÁC (MAE & IoU) =======\n",
    "def compute_iou(boxA, boxB):\n",
    "    xA = max(boxA[0], boxB[0])\n",
    "    yA = max(boxA[1], boxB[1])\n",
    "    xB = min(boxA[2], boxB[2])\n",
    "    yB = min(boxA[3], boxB[3])\n",
    "    interArea = max(0, xB - xA) * max(0, yB - yA)\n",
    "    boxAArea = max(0, (boxA[2] - boxA[0])) * max(0, (boxA[3] - boxA[1]))\n",
    "    boxBArea = max(0, (boxB[2] - boxB[0])) * max(0, (boxB[3] - boxB[1]))\n",
    "    iou = interArea / float(boxAArea + boxBArea - interArea + 1e-6)\n",
    "    return iou\n",
    "\n",
    "all_mae = []\n",
    "all_iou = []\n",
    "num_compared = 0\n",
    "\n",
    "for idx, row in df.iterrows():\n",
    "    frame_num = int(row['frame']) - 1  # -1 để mapping đúng index trong predictions\n",
    "    object_id = row['id']\n",
    "    gt_bbox = [row['x1'], row['y1'], row['x2'], row['y2']]\n",
    "\n",
    "    if object_id in all_predictions and frame_num < len(all_predictions[object_id]):\n",
    "        pred_bbox = all_predictions[object_id][frame_num]\n",
    "        if pred_bbox is not None:\n",
    "            pred_bbox = [float(x) for x in pred_bbox[:4]]\n",
    "            mae = np.mean(np.abs(np.array(gt_bbox) - np.array(pred_bbox)))\n",
    "            all_mae.append(mae)\n",
    "            iou = compute_iou(gt_bbox, pred_bbox)\n",
    "            all_iou.append(iou)\n",
    "            num_compared += 1\n",
    "\n",
    "if num_compared == 0:\n",
    "    print(\"Không có bbox nào được so sánh để tính MAE/IoU!\")\n",
    "else:\n",
    "    mean_mae = np.mean(all_mae)\n",
    "    mean_iou = np.mean(all_iou)\n",
    "    print(f\"\\n===== ĐỘ ĐO ĐÁNH GIÁ TRAJECTORY PREDICTION =====\")\n",
    "    print(f\"- MAE trung bình (pixel): {mean_mae:.2f}\")\n",
    "    print(f\"- IoU trung bình: {mean_iou:.4f}\")\n"
   ]
